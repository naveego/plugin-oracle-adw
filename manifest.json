{
  "id": "plugin-oracle-adw",
  "displayName": "Oracle Autonomous Data Warehouse",
  "description": "Publisher which connects to Oracle's Autonomous Data Warehouse",
  "apiVersion": "2",
  "versionDescription": "Version 2",
  "version": "0.7.1",
  "iconFile": "icon.png",
  "executable": "PluginOracleADW",
  "kind": "publisher",
  "os": "$GOOS",
  "arch": "$GOARCH",
  "accessGroups": ["vandelay", "demo", "casey"],
  "canProduceMultipleSchemas": true,
  "canAcceptQueryBasedSchemas": true,
  "canUseOAuth": false,
  "canWrite": false,
  "canConfigureReplication": true,
  "canWriteReplication": true,
  "querySchema": {
    "ui": {
      "ui:widget": "code",
      "ui:options": {
        "language": "sql"
      }
    },
    "schema": {
      "type": "string"
    }
  },
  "configSchema": {
    "ui": {
      "ui:order": [
        "WalletPath",
        "TNSName",
        "Username",
        "Password"
      ],
      "Password": {
        "ui:widget": "password"
      }
    },
    "schema": {
      "type": "object",
      "properties": {
        "WalletPath" : {
          "type": "string",
          "title": "Wallet Path",
          "description": "The file path to the Oracle Wallet"
        },
        "TNSName": {
          "type": "string",
          "title": "Service Name",
          "description": "The name of the TNS service for connection"
        },
        "Username": {
          "type": "string",
          "title": "Username",
          "description": "The username to use for authentication"
        },
        "Password": {
          "type": "string",
          "title": "Password",
          "description": "The password to use for authentication"
        }
      },
      "required": [
        "WalletPath",
        "Username",
        "Password",
        "TNSName"
      ]
    }
  }
}